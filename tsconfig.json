{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2015",
    "lib": [
      "es6",
      "es2016",
      "es2017",
      "es2021",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/react-bootstrap-mobile/src/types"
    ],
    "declaration": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "strict": false,


    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
  }
}
